generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model About {
  id          Int  @id @default(autoincrement())
  title       Json
  description Json
}

model Blog {
  id           Int     @id @default(autoincrement())
  title        Json
  description  Json
  image        String
  date         String
  author       String
  category     String
  attachment_0 String?
  attachment_1 String?
  attachment_2 String?
  attachment_3 String?
  attachment_4 String?
}

model Client {
  id    Int    @id @default(autoincrement())
  image String
}

model Contact {
  id      Int      @id @default(autoincrement())
  phone   String[]
  email   String[]
  address String[]
  fb      String
  tw      String
  insta   String
  linked  String
  snap    String
}

model Fqa {
  id       Int  @id @default(autoincrement())
  question Json
  answer   Json
}

model Hero {
  id       Int    @id @default(autoincrement())
  title    Json
  subtitle Json
  image    String
}

model Messages {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  phone      String
  email      String
  subject    String
  message    String
}

model NumbersInfo {
  id          Int @id @default(autoincrement())
  service     Int
  customer    Int
  takeService Int
}

model Service {
  id          Int    @id @default(autoincrement())
  image       String
  title       String
  description String
  favoriteNum Int?   @unique
}

model ServiceNumber {
  id        Int @id @default(autoincrement())
  number    Int @unique
  serviceId Int
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  password String
}

model quite {
  id    Int  @id @default(autoincrement())
  quite Json
}
